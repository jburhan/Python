inp = raw_input("Enter scores:")
scr = float(inp)

try:
    if scr < 0.6:
        print "F"
    elif scr < 0.7:
        print "D"
    elif scr < 0.8:
        print "C"
    elif scr < 0.9:
        print "B"
    else:
        print "A"
        
except scr >1.0:
    print "Please insert a numeric value between 0.0 to 1.0"
